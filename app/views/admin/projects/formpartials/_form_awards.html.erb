<div class='row' id="anchor-awards">
  <div class='col-md-12'>
    <div class='panel panel-default'>
      <div class="panel-heading">
        <h3 class="panel-title">Awards</h3>
      </div>
      <div class='panel-body'>
        <div class="row">
          <table class='table'>
            <%= f.fields_for :awards do |builder| %>
              <%= render "admin/projects/formpartials/form_for_award", f:builder %>
            <% end %>
          </table>
        </div>
        <%= link_to_add_fields "Add Award", f, :awards %>

      </div>
    </div>
  </div>
</div>
