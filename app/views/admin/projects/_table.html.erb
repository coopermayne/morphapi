<table class="table table-striped table-hover table-condensed table-bordered index-table">
	<thead>
		<tr>
      <th>title</th>
			<th>design (start)</th>
			<th>design (end)</th>
			<th>constr (start)</th>
			<th>constr (end)</th>
      <th>created</th>
      <th></th>
      <th></th>
		</tr>
	</thead>
	
	<% projects.each do |project| %>
		<tr>
      <td><%= link_to project.title, admin_project_path(project) %>
      <td><%= project.design_sdate %>
      <td><%= project.design_edate %>
      <td><%= project.constr_sdate %>
      <td><%= project.constr_edate %>
      <td><%= project.created_at.strftime("%m/%d/%Y") %>
      <td><%= link_to "#{fa_icon "pencil"}".html_safe, admin_project_path(project) %></td>
      <td><%= link_to "#{fa_icon "trash"}".html_safe, admin_project_path(project), data:  { method: "delete", confirm:  "Are you sure you want to delete this?" } %></td>
		</tr>
	<% end %>
</table>
